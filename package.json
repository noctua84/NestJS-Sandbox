{
  "name": "nestjs-sandbox",
  "version": "0.2.0",
  "description": "This is a sandbox project for NestJS",
  "author": "Markus MÃ¶ller",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:repl": "npm run start -- --entryFile repl",
    "start:dev:repl": "npm run start -- --watch --entryFile repl",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dotenv-vault": "npx dotenv-vault@latest ",
    "vault:new": "dotenv-vault new",
    "vault:push": "dotenv-vault push",
    "vault:pull": "dotenv-vault pull",
    "vault:build": "dotenv-vault build",
    "vault:open": "dotenv-vault open",
    "vault:open:ci": "dotenv-vault open ci",
    "vault:login": "dotenv-vault login",
    "vault:keys:ci": "dotenv-vault keys ci"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/terminus": "^10.1.1",
    "axios": "^1.5.1",
    "dotenv-vault": "^1.25.0",
    "joi": "^17.10.2",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.18",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.6",
    "@types/sinon": "^10.0.17",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.7.0",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "sinon": "^16.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
